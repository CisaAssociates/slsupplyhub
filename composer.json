{
    "name": "slsupplyhub/b2b-platform",
    "description": "B2B e-commerce platform for Southern Leyte suppliers",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.8",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "SLSupplyHub\\": "services/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}